{
  "version": 1,
  "projects": {
    "editor": {
      "root": "apps/editor",
      "sourceRoot": "apps/editor/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/editor",
            "index": "apps/editor/src/index.html",
            "main": "apps/editor/src/main.tsx",
            "polyfills": "apps/editor/src/polyfills.ts",
            "tsConfig": "apps/editor/tsconfig.app.json",
            "assets": ["apps/editor/src/favicon.ico", "apps/editor/src/assets"],
            "styles": ["apps/editor/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/editor/src/environments/environment.ts",
                "with": "apps/editor/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "editor:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "editor:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/editor/.eslintrc",
            "tsConfig": [
              "apps/editor/tsconfig.app.json",
              "apps/editor/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/editor/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/editor/jest.config.js",
            "tsConfig": "apps/editor/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "editor-e2e": {
      "root": "apps/editor-e2e",
      "sourceRoot": "apps/editor-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/editor-e2e/cypress.json",
            "tsConfig": "apps/editor-e2e/tsconfig.e2e.json",
            "devServerTarget": "editor:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "editor:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/editor-e2e/.eslintrc",
            "tsConfig": ["apps/editor-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/editor-e2e/**"]
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/core/.eslintrc",
            "tsConfig": [
              "libs/core/tsconfig.lib.json",
              "libs/core/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "tsConfig": "libs/core/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "editor"
}
